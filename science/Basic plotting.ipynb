    "from matplotlib import pyplot as plt\n",
    "import numpy as np\n",
    "x = np.linspace(0, 2*np.pi + 3)\n",
    "plt.plot(x, np.sin(x))\n",
    "plt.title(\"Sine + shift\")\n",
    "plt.xlabel('time')\n",
    "plt.ylabel('amplitude')"
